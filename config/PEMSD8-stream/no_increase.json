{
    "dataset": "PEMSD8-stream",
    "begin_year": 2012,
    "end_year": 2018,
    "dropout": 0.0,
    "lr": 0.01,
    "batch_size": 128,
    "epoch": 50,
    "gpuid": 1,
    "activation": "relu",
    "scheduler": "epo",
    "momentum":0.99,
    "train": 1,
    "y_len": 12,
    "x_len": 12,
    "data_process": 0,
    "auto_test": 1,
    "auto_lr":1,
    "raw_data_path": "data/PEMSD8-stream/finaldata/",
    "save_data_path": "data/PEMSD8-stream/FastData/",
    "graph_path": "data/PEMSD8-stream/graph/",
    "model_path": "res/PEMSD8-stream/",
    "emb":{
        "input_len":12,
        "input_dim":3,
        "adaptive_emb_dim":32,
        "D^D": 32,
        "D^W":32,
        "D^N":32,
        "output_len":12,
        "num_feat": 1,
        "feature_emb_dim": 128,
        "num_layer":2
    },
    "tcn":{
        "in_channel": 32,
        "out_channel": 32,
        "kernel_size": 3,
        "dilation": 1
    },
    "TMRB":{
        "in_channel":64,
        "out_channel":64,
        "top_k":12,
        "dropout":0.5
    },
    "logname": "no_increase",
    "strategy": "incremental",
    "init": true,
    "increase": false,
    "num_hops": 2,
    
    "replay": true,
    "replay_strategy": "feature",
    "replay_ratio":0.2,

    "is_TMRB":true,
    "is_update":true,
    "select_k":true
}
